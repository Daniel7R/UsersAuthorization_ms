{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "dbConnectionUsers": "${DB_USERS}"
  },
  "APISettings": {
    "JwtOptions": {
      "Secret": "${JWT_SECRET}",
      "Issuer": "${JWT_ISSUER}",
      "Audience": "${JWT_AUDIENCE}"
    }
  },
  "RabbitMQ": {
    "Host": "${HOST_RABBIT}",
    "Port": 5672,
    "Username": "${USERNAME_RABBIT}",
    "Password": "${PASSWORD_RABBIT}"
  }
}

{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    //"dbConnectionUsers": "Server=172.17.56.226; Port=5432; Database= e_sports; user Id=admin; Password=password"
    "dbConnectionUsers": "{{dbUsers}}"
  },
  "APISettings": {
    "JwtOptions": {
      "Secret": "{{Secret}}",
      "Issuer": "{{Issuer}}",
      "Audience": "{{Audience}}"
    }
  },
  "RabbitMQ": {
    "Host": "{{Host}}",
    "Port": 5671,
    "Username": "{{Username}}",
    "Password": "{{Password}}",
    "CertPassphrase": "{{CertPassphrase}}",
    "CertFile": "{{CertFile}}",
    "ServerName": "{{serverName}}"
    //"CaCertPath": "ca-cert.pem"
  }
}
